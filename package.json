{
  "name": "yola-fresh-utils",
  "version": "1.0.2",
  "description": "Utility library for Yola Fresh projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./interfaces": {
      "import": "./dist/interfaces/index.js",
      "require": "./dist/interfaces/index.js",
      "types": "./dist/interfaces/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./data": {
      "import": "./dist/data/index.js",
      "require": "./dist/data/index.js",
      "types": "./dist/data/index.d.ts"
    },
    "./class": {
      "import": "./dist/class/index.js",
      "require": "./dist/class/index.js",
      "types": "./dist/class/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "prepare": "npm run build",
    "generate-exports": "cd scripts && npx ts-node generate-exports.ts && cd ..",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "utilities",
    "typescript",
    "formatting",
    "dates",
    "text"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/yola-fresh-utils.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/yola-fresh-utils/issues"
  },
  "homepage": "https://github.com/yourusername/yola-fresh-utils#readme"
}
